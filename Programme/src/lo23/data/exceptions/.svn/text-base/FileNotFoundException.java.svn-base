package lo23.data.exceptions;



/**
 * This exception class is the one which will be thrown when the program
 * is trying to access a file that doesn't exist, but
 * that is supposed to.
 */
public class FileNotFoundException extends Exception
{
    /**
     * Default constructor, with no specific message
     */
    public FileNotFoundException()
    {
        super();
    }
    
    /**
     * Constructor, with a given message
     * @param message The message contained into the thrown exception
     */
    public FileNotFoundException(String message)
    {
        super(message);
    }
}
